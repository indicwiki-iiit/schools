{# Global Variable  #}
{% set teacherCount={'info':''} %}

{%- macro infobox(title, village, district, state, country, loInt, hiInt, medium, totalStudents) -%}
{{'{{Infobox school'}}
	| name = {{title}}
	| city = {{village}}గ్రామం, {{district}}జిల్లా
	| district = {{district}}జిల్లా
	| state = {{state}}
	| country	 = {{country}}
    | grades_label = తరగతులు
	| grades = {{loInt}} - {{hiInt}}
	| medium = {{medium}} 
	| students = {{totalStudents}}
    | free_label = ఉపాధ్యాయులు
	| free_text = {{teacherCount.info}}
{{'}}\n'}}
{%- endmacro -%}

{%- macro intro(village, district, block, schToken, teMgnt) -%}
	{%- set line =[
			"ఈ "+schToken+village+"గ్రామంలో ఉన్నది. ఈ గ్రామం "+district+"జిల్లాలో "+block+"పరిధిలో ఉన్నది. ",
			"ఈ "+schToken+village+"గ్రామంలో ఉన్నది. "+district+"జిల్లాలో "+block+"పరిధిలో ఈ గ్రామం ఉన్నది. ",
			district+"జిల్లాలో "+block+"పరిధిలో "+village+"గ్రామంలో ఈ "+schToken+"ఉన్నది. ",
			"ఈ "+schToken+district+"జిల్లాలో "+block+"పరిధిలో గల "+village+"గ్రామంలో ఉన్నది. "
		]|random
	-%}
	{{line}}
	{%- if teMgnt|length > 1 -%}
		{%- set line =[
				teMgnt+" నిర్వహణలో ఈ పాఠశాల ఉన్నది. ",
				"ఇది "+teMgnt+" నిర్వహణలో పని చేస్తుంది. "
			]|random
		-%}
		{{line}}
	{%- endif -%}
{%- endmacro -%}

{%- macro classes(fromStd, lo, hi, medium, extraDesc) -%}
	{%- if loInt==1 -%}
		{%- set line =[
			 'ఇక్కడ విద్యార్థులు '+hi+' తరగతి వరకు '+medium+'మాధ్యమంలో విద్యను అభ్యసిస్తారు. ' ,
			 'ఇక్కడ విద్యార్థులకు '+hi+' తరగతి వరకు '+medium+'మాధ్యమంలో బోధన చేస్తారు. ' ,
			 'ఇక్కడ విద్యార్థులకు '+hi+' తరగతి వరకు '+medium+'మాధ్యమంలో చదువు చెప్తారు. '
			]|random
		-%}
	{%- else -%}
		{%- set line =[
			 'ఇందులో '+lo+' తరగతి నుండి '+hi+' తరగతి వరకు '+medium+'మాధ్యమంలో భోదించబడుతుంది. ' ,
			 'ఇందులో విద్యార్థులు '+lo+' తరగతి నుండి '+hi+' తరగతి వరకు '+medium+'మాధ్యమంలో విద్యను అభ్యసిస్తారు. ' ,
			 'ఇందులో '+lo+' తరగతి నుండి '+hi+' తరగతి వరకు '+medium+'మాధ్యమంలో చదువు చెప్తారు. '
			]|random
		-%}
	{%- endif -%}
	{%- if extraDesc|length > 1 -%}
		{{extraDesc}} {{line}}
	{%- else -%}
		{{line}}
	{%- endif -%}
{%- endmacro -%}

{%- macro codeAndManagement(code) -%}
==గుర్తింపు==
    {{- "\n" -}}
	{{"ఈ పాఠశాల ఏకీకృత జిల్లా సమాచార విద్యా వ్యవస్థ ([http://udise.in/ U-DISE]) కోడ్ "+code+". "}}
