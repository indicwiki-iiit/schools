{%- macro infobox(title, village, district, state, country, loInt, hiInt, medium, totalStudents, totalTeachers) -%}
	{{'{{Infobox school'}}
		| name		= {{title}}
		| city			= {{village}}, {{district}}
		| district		= {{district}}
		| state		= {{state}}
		| country		= {{country}}
		| grades		= {{loInt}} - {{hiInt}}
		| medium	= {{medium}} 
		| studnets	= {{totalStudents}}
		| faculty		= {{totalTeachers}}
	{{'}}\n'}}
{%- endmacro -%}

{%- macro intro(village, district, block, code) -%}
	{%- set line =[
			"ఈ పాఠశాల "+village+" గ్రామంలో ఉన్నది. ఈ గ్రామం "+district+" జిల్లాలో "+block+" పరిధి నందు ఉన్నది. ",
			"ఈ పాఠశాల "+village+" గ్రామంలో ఉన్నది. "+district+" జిల్లా నందు "+block+" పరిధిలో ఈ గ్రామం ఉన్నది. ",
			district+"జిల్లాలో "+block+" పరిధిలో "+village+" గ్రామం నందు ఈ పాఠశాల ఉన్నది. ",
			"ఈ పాఠశాల "+district+" జిల్లాలో "+block+" పరిధిలో గల "+village+" గ్రామము నందు ఉన్నది. "
		]|random
	-%}
	{{line+"ఈ పాఠశాల యొక్క ఏకీకృత జిల్లా సమాచార విద్యా వ్యవస్థ ([http://udise.in/ U-DISE]) కోడ్ "+code+". "}}
{%- endmacro -%}

{%- macro management(teMgnt) -%}
	{%- set line =[
			teMgnt+" నిర్వహణలో ఈ పాఠశాల ఉన్నది. ",
			"ఇది "+teMgnt+" నిర్వహణలో పని చేస్తుంది. "
		]|random
	-%}
	{{line}}
{%- endmacro -%}

{%- macro classes(fromStd, lo, hi) -%}
	{%- if loInt==1 -%}
		{{ 'ఇక్కడ విద్యార్థులు '+hi+' తరగతి వరకూ విద్యని అభ్యసించవచ్చు. ' }}
	{%- else -%}
		{{ 'ఇందులో '+lo+' తరగతి నుండి '+hi+' తరగతి వరకు విద్యాబోధన కలదు. ' }}
	{%- endif -%}
{%- endmacro -%}

{%- macro schMedium(medium) -%}
	{%- set line =[
			medium+" మాధ్యమంలో ఇక్కడ పాఠములు బోధిస్తారు. ",
			"ఇక్కడ "+medium+" మాధ్యమంలో పాఠములు బోధిస్తారు. ",
			medium+" మాధ్యమంలో ఇక్కడ పాఠాలు బోధిస్తారు. ",
			"ఇక్కడ "+medium+" మాధ్యమంలో పాఠాలు బోధిస్తారు. "
		]|random
	-%}
	{{line}}
{%- endmacro -%}


{%- macro studentsCount(sType, bCount, gCount, totalStudents, spBCount, spGCount) -%}
	{%- if sType=='co-educational' -%}
		{%- set desc='ఇది ఒక బాల బాలికల పాఠశాల. ' -%}
		{%- set counts=[
				'ఇక్కడ '+bCount+' బాలురు '+gCount+' బాలికలు, మొత్తం '+totalStudents+' విద్యార్థులు చదువుతున్నారు. ',
				'ఈ పాఠశాల లో మొత్తం '+totalStudents+' విద్యార్థులు చదువుతున్నారు. వీరిలో '+bCount+' బాలురు '+gCount+' బాలికలు ఉన్నారు. ',
				'మొత్తం '+totalStudents+' విద్యార్థులలో '+bCount+' బాలురు '+gCount+' బాలికలు ఈ పాఠశాల లో చదువుతున్నారు. '
			]|random
		-%}
	{%- elif sType=='boys' -%}
		{%- set desc=[
				'ఇది ఒక బాలుర పాఠశాల. ' ,
				'ఇక్కడ బాలురకు మాత్రమే ప్రవేశం ఉన్నది. ',
				'బాలురకు మాత్రమే ఇక్కడ ప్రవేశం ఉన్నది. '
			]|random
		-%}
		{%- set counts=[
				'ఇక్కడ చదువుతున్న బాలుర సాంఖ్య '+spBCount+'. ',
				'ఇక్కడ '+bCount+' బాలురు చదువుతున్నారు. ',
				bCount+' బాలురు ఇక్కడ చదువుతున్నారు. '
			]|random
		-%}
	{%- elif sType=='girls' -%}
		{%- set desc=[
				'ఇది ఒక బాలికల పాఠశాల. ' ,
				'ఇక్కడ బాలికలకు మాత్రమే ప్రవేశం ఉన్నది. ',
				'బాలికలకు మాత్రమే ఇక్కడ ప్రవేశం ఉన్నది. '
			]|random
		-%}
		{%- set counts=[
				'ఇక్కడ చదువుతున్న బాలికల సాంఖ్య '+spGCount+'. ',
				'ఇక్కడ '+gCount+' బాలికలు చదువుతున్నారు. ',
				gCount+' బాలికలు ఇక్కడ చదువుతున్నారు. '
			]|random
		-%}
	{%- endif -%}
	{{desc}}{{counts}}
{%- endmacro -%}

{%- macro teachersCount(mInt, mCount, fInt, fCount, totalTeachers) -%}
	{%- if mInt==1 and fInt==0 -%}
		{%- set line='ఇది ఒక ఏకోపాధ్యాయ పాఠశాల. ఒక ఉపాధ్యాయుడు ఉన్నారు.' -%}
	{%- elif mInt>1 and fInt==0 -%}
		{%- set line='ఇక్కడ '+mCount+' ఉపాధ్యాయులు ఉన్నారు. ' -%}
	{%- elif mInt==0 and fInt==1 -%}
		{%- set line='ఇది ఒక ఏకోపాధ్యాయ పాఠశాల. ఒక ఉపాధ్యాయిని ఉన్నారు. ' -%}
	{%- elif mInt==0 and fInt>1 -%}
		{%- set line='ఇక్కడ '+fCount+' ఉపాధ్యాయులు ఉన్నారు, వీరిలో అందరూ ఉపాధ్యాయినులే. ' -%}
	{%- else -%}
		{%- set line=[
				'ఇక్కడ '+mCount+' ఉపాధ్యాయులు '+fCount+' ఉపాధ్యాయినులే, మొత్తం '+totalTeachers+' ఉపాధ్యాయులు ఉన్నారు. ',
				'ఇక్కడ మొత్తం '+totalTeachers+' ఉపాధ్యాయులు ఉన్నారు. వీరిలో '+mCount+' ఉపాధ్యాయులు '+fCount+' ఉపాధ్యాయినులు ఉన్నారు. ',
				'మొత్తం '+totalTeachers+' ఉపాధ్యాయులు '+mCount+' ఉపాధ్యాయులు '+fCount+' ఉపాధ్యాయినులు ఇక్కడ ఉన్నారు. '
			]|random
		-%}
	{%- endif -%}
	{{line}}
{%- endmacro -%}



{%- macro ending() -%}
	{{'&lt;ref&gt;{{cite web |title =School Directory |url =http://udise.in/school_directory.htm |website = udise.in }}&lt;/ref&gt;. ఈ వివరములు నేషనల్ యూనివర్శిటీ ఆఫ్ ఎడ్యుకేషనల్ ప్లానింగ్ అండ్ అడ్మినిస్ట్రేషన్ ([https://www.indiaeducation.net/apexbodies/nuepa/ NUEPA]) జరిపిన సర్వే ఆధారముగా 2011-12 విద్యా సంవత్సరమునకు సంబంధించినవి.'}}

	==మూలాలు==
	{{'{{Reflist}}'}}

	[[Category:School]]
{%- endmacro -%}

{%- macro descLine(enMgnt, enName) -%}
	{%- if 'KGBV' in enName -%}
		{%- set line='ఇది ఒక కస్తూర్బా గాంధీ బాలికా విద్యాలయ (KGBV). ' -%}
	{%- elif 'MPPS' in enName -%}
		{%- set line='ఇది ఒక మండల ప్రజా పరిషత్ ప్రాథమిక పాఠశాల (MPPS). ' -%}
	{%- elif 'MPUPS' in enName -%}
		{%- set line='ఇది ఒక మండల ప్రజా పరిషత్ ప్రాథమికోన్నత పాఠశాల (MPUPS). ' -%}
	{%- elif 'ZPPS' in enName -%}
		{%- set line='ఇది ఒక జిల్లా పరిషత్ ప్రాథమిక పాఠశాల (ZPPS). ' -%}
	{%- elif 'ZPHS' in enName -%}
		{%- set line='ఇది ఒక జిల్లా పరిషత్ ఉన్నత పాఠశాల (ZPHS). ' -%}
	{%- elif 'Pvt.Aided' in enMgnt -%}
		{%- set line='ఇది ఒక ప్రైవేటు ఎయిడెడ్ పాఠశాల. ' -%}
	{%- elif 'Pvt.Unaided' in enMgnt -%}
		{%- set line='ఇది ఒక ప్రైవేటు పాఠశాల. ' -%}
	{%- else -%}
		{%- set line='' -%}
	{%- endif -%}
	{{line}}
{%- endmacro -%}



